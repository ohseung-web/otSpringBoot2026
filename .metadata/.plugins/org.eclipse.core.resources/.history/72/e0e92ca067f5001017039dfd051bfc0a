package myCalculator;

public class CalA {
  //IoC 만들어 보기
 
  MyCalculator mycal;
  CalAdd caladd;
  CalAmul calmul;
  CalAsub calsub;
  CalDiv caldiv;
  
  //생성자가 초기화될떄 객체가 생성
  public CalA() {
	  //
	  this.mycal = new MyCalculator();
	  this.caladd = new CalAdd();
	  this.calmul = new CalAmul();
	  this.caldiv = new CalDiv();
	  this.calsub = new CalAsub();
  }
}
